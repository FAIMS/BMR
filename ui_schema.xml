        </trigger>
        <trigger ref="newRopeGroove">
          <label>New Rope Groove</label>
        </trigger>
        <trigger ref="Other">
          <label>Other</label>
        </trigger>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>Search</label>
        <input faims_annotation="false" faims_certainty="false" faims_read_only="false" ref="Search_Bar">
          <label>Search Term</label>
        </input>
        <group faims_style="orientation" ref="container1">
          <label/>
          <group faims_style="even" ref="child1">
            <label>2</label>
            <select1 faims_annotation="false" faims_certainty="false" ref="Dropdown">
              <label>Select Entity:</label>
              <item>
                <label>placeholder</label>
                <value>placeholder</value>
              </item>
            </select1>
          </group>
          <group faims_style="leftBigger" ref="child2">
            <label/>
            <group ref="child1">
              <label/>
              <trigger ref="searchButton">
                <label>Search</label>
              </trigger>
            </group>
            <group ref="child2">
              <label/>
              <trigger ref="searchAllButton">
                <label>Search All</label>
              </trigger>
            </group>
          </group>
        </group>
        <select1 appearance="compact" faims_annotation="false" faims_certainty="false" ref="Search_Results">
          <label>Tap Record ID in list below to load it.</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
      <group ref="gps">
        <label>Control</label>
        <trigger ref="connectexternal">
          <label>Connect To External GPS</label>
        </trigger>
        <trigger ref="connectinternal">
          <label>Connect To Internal GPS</label>
        </trigger>
        <trigger ref="startsync">
          <label>Start Synching</label>
        </trigger>
        <trigger ref="stopsync">
          <label>Stop Synching</label>
        </trigger>
      </group>
    </group>
    <group ref="Bucket" faims_archent_type="bucket">
