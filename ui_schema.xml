  <h:head>
    <h:title>Ropeway</h:title>
    <!--model lists user interface tabgroups, tabs and content of tabs-->
    <model>
      <instance>
        <faims id="ropeway">
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <leftBigger>
              <layout_weight/>
            </leftBigger>
          </style>
          <user>
            <usertab>
              <users/>
            </usertab>
          </user>
          <control>
            <New_Data>
              <newTower/>
              <newTensionStation/>
              <newBucket/>
              <newTrackRope/>
              <newHaulRope/>
              <newRopeJoiner/>
              <newRopeGroove/>
              <Other/>
            </New_Data>
            <Search>
              <Search_Bar/>
              <container1>
                <child1>
                  <Dropdown/>
                </child1>
                <child2>
                  <child1>
                    <searchButton/>
                  </child1>
                  <child2>
                    <searchAllButton/>
                  </child2>
                </child2>
              </container1>
              <Search_Results/>
            </Search>
            <gps>
              <connectexternal/>
              <connectinternal/>
              <startsync/>
              <stopsync/>
            </gps>
          </control>
          <Bucket>
            <Carrier_Details>
              <Bucket_assembly_number/>
              <Fitted_with_anti-tip_brackets/>
              <Grip_slide_present/>
              <Bolt_in_grip_slide/>
              <Grip_slide_smashed/>
              <Grip_present/>
              <Idler_wheel_present/>
              <Idler_wheel_grease_cap_present/>
              <LH_diagonal/>
              <LH_carrier_arm/>
              <RH_diagonal/>
              <RH_carrier_arm/>
              <Carrier_notes/>
              <Carrier_latitude/>
              <Carrier_longitude/>
              <CalculateGPS/>
              <Carrier_photos/>
              <takePhoto/>
              <Update/>
              <Delete/>
            </Carrier_Details>
            <PhotoLog/>
            <Carriage_Details>
              <Left_track_damage/>
              <Left_track_rope_wheel/>
              <Right_track_damage/>
              <Right_track_rope_wheel/>
              <Number_of_bolts/>
              <Carriage_notes/>
              <Carriage_latitude/>
              <Carriage_longitude/>
              <CalculateGPS/>
              <Carriage_photos/>
              <takePhoto/>
              <Update/>
              <Delete/>
            </Carriage_Details>
            <Bucket_Details>
              <Hit/>
              <Bucket_notes/>
              <Bucket_latitude/>
              <Bucket_longitude/>
              <CalculateGPS/>
              <Bucket_photos/>
              <takePhoto/>
              <Update/>
              <Delete/>
            </Bucket_Details>
          </Bucket>
          <Tower>
            <Tower_pole>
              <Tower_number/>
              <Fourth_stabilising_pole/>
              <Add_pole/>
              <List_poles/>
              <Show_pole/>
              <Delete_pole/>
              <Footing_spacing/>
              <Tower_pole_notes/>
              <Update/>
              <Delete/>
            </Tower_pole>
            <Tower_hardware>
              <Tower_type/>
              <Add_track_crossbar/>
              <List_track_crossbar/>
              <Show_track_crossbar/>
              <Delete_track_crossbar/>
              <Haul_rope_cross_timber/>
              <Add_haul_rope_wheel_frame/>
              <List_haul_rope_wheel_frame/>
              <Load_haul_rope_wheel_frame/>
              <Delete_haul_rope_wheel_frame/>
              <Tower_hardware_notes/>
              <Update/>
              <Delete/>
            </Tower_hardware>
            <Tower_bolts>
              <Add_tower_bolt/>
              <List_tower_bolt/>
              <Update/>
              <Delete/>
            </Tower_bolts>
          </Tower>

          <Tower_pole>
            <Details>
              <Pole_number/>
              <Pole_exists/>
              <Length/>
              <Diameter/>
              <Orientation/>
              <Footing/>
              <Footing_depth/>
              <Footing_diameter/>
              <Latitude/>
              <Longitude/>
              <Photos/>
              <Attach_photos/>
              <Update/>
              <Delete/>
            </Details>
          </Tower_pole>

          <Track_crossbar>
            <Details>
              <Crossbar_number/>
              <Track_rope_wheel_cross_bar/>
              <Track_rope_cross_timber_present/>
              <U_clamps/>
              <Track_rope_saddle_or_wheel/>
              <Head_plate/>
              <Cross_timber_angle_bracket/>
              <Latitude/>
              <Longitude/>
              <Photos/>
              <Attach_photos/>
              <Update/>
              <Delete/>
            </Details>
          </Track_crossbar>

          <Wheel_frame>
            <Details>
              <Frame_number/>
              <Wheel/>
              <Bearings/>
              <Latitude/>
              <Longitude/>
              <Photos/>
              <Attach_photos/>
              <Update/>
              <Delete/>
            </Details>
          </Wheel_frame>

          <Tower_bolts>
            <Details>
              <Length/>
              <Diameter/>
              <Type/>
              <Head/>
              <In_situ/>
              <Function/>
              <Estimated_offset/>
              <Estimated_distance/>
              <Nut/>
              <Washer/>
              <Length_of_bolt/>
              <Notes/>
              <Latitude/>
              <Longitude/>
              <Photos/>
              <Attach_photos/>
              <Update/>
              <Delete/>
            </Details>
          </Tower_bolts>

        </faims>
      </instance>
   <!--     <bind nodeset="" type=""/>   -->
    </model>
  </h:head>

    <h:body>
    <!--Describes controls for each tabgroup and its nested tabs-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="leftBigger">
        <label/>
        <input ref="layout_weight">
          <label>2</label>
        </input>
      </group>
    </group>
    <group ref="user">
      <label>User List</label>
      <group ref="usertab" faims_scrollable="false">
        <label>User_List</label>
        <select1 appearance="compact" ref="users">
          <label>Users:</label>
          <item>
            <label>placeholder</label>
            <value>placeholder</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="control">
      <label>Bleichert Ropeway</label>
      <group ref="New_Data" faims_scrollable="false">
        <label>Record New Data</label>
        <trigger ref="newTower">
          <label>New Tower</label>
        </trigger>
        <trigger ref="newTensionStation">
          <label>New Tension Station</label>
        </trigger>
        <trigger ref="newBucket">
          <label>New Bucket</label>
        </trigger>
        <trigger ref="newTrackRope">
          <label>New Track Rope</label>
        </trigger>
        <trigger ref="newHaulRope">
